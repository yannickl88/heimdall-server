{
    "name": "yannickl88/heimdall",
    "authors": [
        {
            "name": "Yannick de Lange",
            "email": "yannick.l.88@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/console": "^3.4",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.4",
        "symfony/yaml": "^3.4",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/security-bundle": "^3.4"
    },
    "require-dev": {
        "symfony/dotenv": "^3.4",
        "symfony/profiler-pack": "^1.0",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": [
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    }
}
